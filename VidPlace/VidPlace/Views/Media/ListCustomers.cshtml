@model VidPlace.ViewModels.customerMediaViewModel
@{
    ViewBag.Title = "ListCustomers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movie: @Model.media.Name</h2>
Customer List:
@if (Model.customersList.Count == 0)
{
<h3>No customers</h3>
}
else
{
<ul>
@foreach (var data in Model.customersList)
{
    <li>@data.Name</li>
}

</ul>
}